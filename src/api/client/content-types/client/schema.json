{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "birthDate": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Masculino",
        "Feminino"
      ]
    },
    "cpf": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "typePhone": {
      "type": "enumeration",
      "enum": [
        "Celular",
        "Fixo"
      ]
    },
    "ranking": {
      "type": "integer"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "client"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "client"
    },
    "cards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::card.card",
      "mappedBy": "client"
    }
  }
}
